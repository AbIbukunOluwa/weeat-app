<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
</head>
<body>
  <h1>Contact Messages</h1>
  <ul>
    <% messages.forEach(msg => { %>
      <li>
        <strong><%= msg.name %></strong> (<%= msg.email %>)<br>
        <%= msg.message %><br>
        <small><%= msg.createdAt.toLocaleString() %></small>
      </li>
    <% }) %>
  </ul>
  <p><a href="/admin">Back to Admin</a></p>
</body>
</html>
